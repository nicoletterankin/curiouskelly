{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "astro",
  "rewrites": [
    {
      "source": "/es-es/:path*",
      "destination": "/es-es/:path*"
    },
    {
      "source": "/pt-br/:path*",
      "destination": "/pt-br/:path*"
    }
  ]
}










